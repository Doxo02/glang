// argc: i32, argv: char** 
fn main(argv: char**, argc: i64) -> i64 {
    hello();
    let x: i64 = add(some_int(), 1);
    return x;
}

fn hello() -> void {
    syscall(1, 0, "Hello World\n", 13, 0, 0, 0);
}

fn some_int() -> i64 {
    return 5;
}

fn add(a: i64, b: i64) -> i64 {
    return a - b;
}
